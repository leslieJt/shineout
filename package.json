{
  "name": "shineout",
  "version": "1.1.4",
  "description": "Shein 前端组件库",
  "main": "./lib/index.js",
  "author": "lobos841@gmail.com",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/",
    "start": "cross-env NODE_ENV=development node dev-server.js",
    "css-module": "cross-env NODE_ENV=development LOCAL_IDENT_NAME=[local]-[hash:base64:5] node dev-server.js",
    "docs-build-html": "node scripts/build-html.js",
    "docs-build-js": "cross-env NODE_ENV=production webpack --config webpack/config.doc.js",
    "docs-build": "npm-run-all docs-build-*",
    "build-clean": "rm -rf publish/es publish/dist publish/lib publish/css",
    "build-index": "node scripts/src-index.js",
    "build-package": "node scripts/build-package.js",
    "build-dist-dev": "cross-env NODE_ENV=development webpack --config webpack/config.dist.dev.js",
    "build-dist": "cross-env NODE_ENV=production webpack --config webpack/config.dist.js",
    "build-readme": "cp README.md publish/README.md",
    "build-es6": "cp -R src publish/es",
    "build-less": "mkdir publish/lib && cp -R src/styles publish/lib/styles",
    "build-es5": "cross-env NODE_ENV=publish babel src --out-dir publish/css && cp -R publish/css/* publish/lib",
    "build-css": "node scripts/build-css.js",
    "build": "npm-run-all build-*"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "classnames": "^2.2.5",
    "create-react-context": "^0.2.3",
    "date-fns": "2.0.0-alpha.10",
    "deep-eql": "^3.0.1",
    "immer": "^1.1.1",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^7.2.4",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "chokidar": "^2.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "ejs": "^2.5.7",
    "es6-object-assign": "^1.1.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "http-proxy": "^1.16.2",
    "jszip": "^3.1.5",
    "koa": "^2.4.1",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.3.0",
    "koa-send": "^4.1.2",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.5",
    "raw-loader": "^0.5.1",
    "react": "^16.6.3",
    "react-color": "^2.14.0",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.2",
    "react-loadable": "^5.3.1",
    "react-markdown": "^3.1.4",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "2.11.2",
    "webpack-merge": "^4.1.2",
    "whatwg-fetch": "^2.0.4"
  }
